<% form_for MaterialInout.search,:url => {:action => :index,:controller => :all_invoice_shippments},:html => {:class => :uniForm } do |f| -%>
  <fieldset class='blockLabels'>
    <%=hidden_field_tag :active_sub_tab,params[:active_sub_tab]%>
    <div class='ctrlHolder'>
      <%=f.label :org_id_eq,"领用部门"%>
      <%=f.select :org_id_eq,orgs_for_select,:include_blank => true %>
    </div>

    <div class='ctrlHolder'>
      <%=f.label :person_name_like,"经手人"%>
      <%=f.text_field :person_name_like,:class => "textInput" %>
    </div>
    <div class='ctrlHolder'>
      <%=f.label :inout_date_gte,"出库日期"%>
      <%=f.calendar_date_select :inout_date_gte,:class => "textInput" %>
    </div>
    <div class='ctrlHolder'>
      <%=f.label :inout_date_lte,"~"%>
      <%=f.calendar_date_select :inout_date_lte,:class => "textInput" %>
    </div>
  </fieldset>
  <div class="actions-bar">
    <%=f.submit '查询', :class => 'sexybutton sexysimple sexyblue'%>
  </div>
<% end -%>
