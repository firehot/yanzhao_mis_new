<% form_for SalaryTableLine.search,:url => {:controller =>:salary_tables,:action => :index_sum},:html => {:class => :uniForm } do |f| -%>
  <fieldset class='inlineLabels'>
    <!--无效员工不显示-->
    <%= f.hidden_field :employee_is_active_eq,:value => true %>
    <div class='ctrlHolder'>
      <%=f.label :salary_table_mth_lte,"月份"%>
      <%=f.select :salary_table_mth_lte,months_for_select,:include_blank => true %>
    </div>
    <div class='ctrlHolder'>
      <%=f.label :employee_id_eq,"员工"%>
      <%=f.select :employee_id_eq,employees_for_select,:include_blank => true %>
    </div>

  </fieldset>
  <div class="actions-bar">
    <%=f.submit '出勤统计', :class => 'sexybutton sexysimple sexyblue'%>
  </div>
<% end %>
